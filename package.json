{
  "name": "word-to-markdown",
  "version": "0.0.0",
  "description": "Convert Word documents to beautiful Markdown.",
  "type": "module",
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/jest": "~29.5",
    "@types/node": "~24",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "~6.15",
    "@typescript-eslint/parser": "~6.15",
    "css-loader": "^7.1.2",
    "eslint": "~8.57",
    "eslint-config-prettier": "~10.1",
    "eslint-plugin-jest": "~27.6",
    "jest": "~29.7",
    "playwright": "^1.55.1",
    "prettier": "~3.6",
    "style-loader": "^4.0.0",
    "ts-api-utils": "~2.1",
    "ts-jest": "~29.4",
    "ts-loader": "^9.5.4",
    "typescript": "~5.9",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "scripts": {
    "fix": "eslint --fix --ext .ts . && prettier --write .",
    "build": "npm run build:js && npm run build:pages && npm run build:web",
    "build:js": "tsc",
    "build:pages": "node scripts/build-pages.js",
    "build:web": "webpack build",
    "server:web": "webpack serve --open",
    "lint": "eslint . --ext .ts --ext .mts && prettier --check .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "all": "npm run fix && npm run test && npm run build && check-builds",
    "server": "node build/server.js",
    "check-builds": "./scripts/check-builds.sh"
  },
  "author": "Ben Balter <ben@balter.com>",
  "license": "ISC",
  "dependencies": {
    "@joplin/turndown": "^4.0.81",
    "@joplin/turndown-plugin-gfm": "^1.0.63",
    "@types/helmet": "^4.0.0",
    "bootstrap": "^5.3.8",
    "clipboard": "^2.0.11",
    "commander": "^14.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mammoth": "^1.11.0",
    "markdownlint": "^0.38.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-html-parser": "^7.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tslib": "~2.8",
    "unified": "^11.0.5",
    "url": "^0.11.4"
  },
  "volta": {
    "node": "20.10.0"
  },
  "bin": {
    "w2m": "build/cli.js"
  },
  "main": "build/main.js"
}
